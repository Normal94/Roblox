    for _, v in pairs(workspace:GetDescendants()) do
        if (v.Name == loadstring(base64decode("Q29pbg=="))() or v.Name == loadstring(base64decode("Q3JlZGl0"))() or v.Name == loadstring(base64decode("TW9uZXk="))()) and (v:IsA(loadstring(base64decode("QmFzZVBhcnQ="))()) or v:IsA(loadstring(base64decode("TWVzaFBhcnQ="))())) then
