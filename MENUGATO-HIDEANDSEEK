local function decodificarBase64(data)
    local b = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/'
    data = string.gsub(data, '[^'..b..'=]', '')
    return (data:gsub('.', function(qejiymwx)
        if (qejiymwx == '=') then return '' end
        local r,f='',(b:find(qejiymwx)-1)
        for muabZUa9=6,1,-1 do r=r..(f%2^muabZUa9-f%2^(muabZUa9-1)>0 and '1' or '0') end
        return r;
    end):gsub('%d%d%d?%d?%d?%d?%d?%d?', function(qejiymwx)
        if (#qejiymwx ~= 8) then return '' end
        local c=0
        for muabZUa9=1,8 do c=c+(qejiymwx:sub(muabZUa9,muabZUa9)=='1' and 2^(8-muabZUa9) or 0) end
        return string.char(c)
    end))
end

if not isfile(nombreLogo) then 
    local s, r = pcall(function() return game:HttpGet(urlLogo) end)
    if s and r then writefile(nombreLogo, decodificarBase64(string.gsub(r, loadstring(base64decode("JXMr"))(), loadstring(base64decode(""))()))) end
end
if not isfile(nombreAudio) then 
    local s, r = pcall(function() return game:HttpGet(urlAudio) end)
    if s and r then writefile(nombreAudio, r) end
end

