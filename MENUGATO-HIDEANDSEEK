local Library = {}

function Library:CreateWindow(WindowName)
    local ScreenGui = Instance.new(loadstring(base64decode("U2NyZWVuR3Vp"))())
    ScreenGui.Name = loadstring(base64decode("TWVudUdhdG9VSQ=="))()
    ScreenGui.ResetOnSpawn = false
    ScreenGui.IgnoreGuiInset = true
    ScreenGui.Parent = CoreGui

    local fadeInfo = TweenInfo.new(0.6, Enum.EasingStyle.Quint, Enum.EasingDirection.Out)
    local tweenInfo = TweenInfo.new(0.4, Enum.EasingStyle.Quint, Enum.EasingDirection.Out)

    -- PANTALLA DE CARGA
    local LoadingFrame = Instance.new(loadstring(base64decode("RnJhbWU="))(), ScreenGui)
    LoadingFrame.Size = UDim2.new(1, 0, 1, 0)
    LoadingFrame.BackgroundColor3 = Color3.fromRGB(12, 12, 12)
    LoadingFrame.BackgroundTransparency = 1
    LoadingFrame.ZIndex = 100

    local LogoImage = Instance.new(loadstring(base64decode("SW1hZ2VMYWJlbA=="))(), LoadingFrame)
    LogoImage.Size = UDim2.new(0, 160, 0, 160)
    LogoImage.Position = UDim2.new(0.5, -80, 0.5, -100)
    LogoImage.BackgroundTransparency = 1
    LogoImage.Image = isfile(nombreLogo) and getcustomasset(nombreLogo) or loadstring(base64decode("cmJ4YXNzZXRpZDovLzc1ODU3NjU0MDIzMTA5"))()
    LogoImage.ImageTransparency = 1
    LogoImage.ScaleType = Enum.ScaleType.Fit
    LogoImage.ZIndex = 101

    local LoadingText = Instance.new(loadstring(base64decode("VGV4dExhYmVs"))(), LoadingFrame)
    LoadingText.Size = UDim2.new(0, 200, 0, 30)
    LoadingText.Position = UDim2.new(0.5, -100, 0.5, 75)
    LoadingText.BackgroundTransparency = 1
    LoadingText.Text = loadstring(base64decode("Q2FyZ2FuZG8uLi4="))()
    LoadingText.TextColor3 = Color3.fromRGB(255, 255, 255)
    LoadingText.Font = Enum.Font.GothamBold
    LoadingText.TextSize = 16
    LoadingText.TextTransparency = 1
    LoadingText.ZIndex = 101

    -- BOTÓN MINIMIZADO
    local OpenButton = Instance.new(loadstring(base64decode("VGV4dEJ1dHRvbg=="))(), ScreenGui)
    OpenButton.Name = loadstring(base64decode("T3BlbkJ1dHRvbg=="))()
    OpenButton.Size = UDim2.new(0, 100, 0, 26)
    OpenButton.Position = UDim2.new(0.5, -50, 0, 5)
    OpenButton.BackgroundColor3 = Color3.fromRGB(20, 25, 30)
    OpenButton.BackgroundTransparency = 1
    OpenButton.Text = loadstring(base64decode("TUVOw5ogR0FUTw=="))()
    OpenButton.TextColor3 = Color3.fromHex(loadstring(base64decode("I2Y5YmIzNQ=="))())
    OpenButton.Font = Enum.Font.Gotham
    OpenButton.TextSize = 12
    OpenButton.TextTransparency = 1
    OpenButton.Visible = false
    Instance.new(loadstring(base64decode("VUlDb3JuZXI="))(), OpenButton).CornerRadius = UDim.new(0, 8)
    local OpenStroke = Instance.new(loadstring(base64decode("VUlTdHJva2U="))(), OpenButton)
    OpenStroke.Color = Color3.fromHex(loadstring(base64decode("I2Y5YmIzNQ=="))())
    OpenStroke.Transparency = 1

    -- VENTANA PRINCIPAL
    local MainFrame = Instance.new(loadstring(base64decode("Q2FudmFzR3JvdXA="))(), ScreenGui)
    MainFrame.Size = UDim2.new(0, 480, 0, 380)
    MainFrame.Position = UDim2.new(0.5, -240, 0.5, -190)
    MainFrame.BackgroundColor3 = Color3.fromRGB(15, 20, 25)
    MainFrame.BackgroundTransparency = 0.3
    MainFrame.GroupTransparency = 1
    MainFrame.Visible = false
    Instance.new(loadstring(base64decode("VUlDb3JuZXI="))(), MainFrame).CornerRadius = UDim.new(0, 12)
    local MainScale = Instance.new(loadstring(base64decode("VUlTY2FsZQ=="))(), MainFrame)
    MainScale.Scale = 0.8

    local TopBar = Instance.new(loadstring(base64decode("RnJhbWU="))(), MainFrame)
    TopBar.Size = UDim2.new(1, 0, 0, 40)
    TopBar.BackgroundTransparency = 1

    local Title = Instance.new(loadstring(base64decode("VGV4dExhYmVs"))(), TopBar)
    Title.Size = UDim2.new(1, -50, 1, 0)
    Title.Position = UDim2.new(0, 15, 0, 0)
    Title.BackgroundTransparency = 1
    Title.Text = WindowName or loadstring(base64decode("TUVOw5ogR0FUTw=="))()
    Title.TextColor3 = Color3.fromRGB(255, 255, 255)
    Title.Font = Enum.Font.GothamBold
    Title.TextSize = 16
    Title.TextXAlignment = Enum.TextXAlignment.Left

    local CloseBtn = Instance.new(loadstring(base64decode("VGV4dEJ1dHRvbg=="))(), TopBar)
    CloseBtn.Size = UDim2.new(0, 30, 0, 30)
    CloseBtn.Position = UDim2.new(1, -40, 0, 5)
    CloseBtn.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    CloseBtn.BackgroundTransparency = 0.6
    CloseBtn.Text = loadstring(base64decode("WA=="))()
    CloseBtn.TextColor3 = Color3.fromRGB(200, 200, 200)
    CloseBtn.Font = Enum.Font.GothamBold
    CloseBtn.TextSize = 16
    Instance.new(loadstring(base64decode("VUlDb3JuZXI="))(), CloseBtn).CornerRadius = UDim.new(0, 8)

    local TabBar = Instance.new(loadstring(base64decode("U2Nyb2xsaW5nRnJhbWU="))(), MainFrame)
    TabBar.Size = UDim2.new(1, -20, 0, 35)
    TabBar.Position = UDim2.new(0, 10, 0, 45)
    TabBar.BackgroundTransparency = 1
    TabBar.ScrollBarThickness = 0
    local TabBarLayout = Instance.new(loadstring(base64decode("VUlMaXN0TGF5b3V0"))(), TabBar)
    TabBarLayout.FillDirection = Enum.FillDirection.Horizontal
    TabBarLayout.Padding = UDim.new(0, 8)
    TabBarLayout.SortOrder = Enum.SortOrder.LayoutOrder

    TabBarLayout:GetPropertyChangedSignal(loadstring(base64decode("QWJzb2x1dGVDb250ZW50U2l6ZQ=="))()):Connect(function()
        TabBar.CanvasSize = UDim2.new(0, TabBarLayout.AbsoluteContentSize.X, 0, 0)
    end)

    local PageContainer = Instance.new(loadstring(base64decode("RnJhbWU="))(), MainFrame)
    PageContainer.Size = UDim2.new(1, -20, 1, -95)
    PageContainer.Position = UDim2.new(0, 10, 0, 85)
    PageContainer.BackgroundTransparency = 1

    local dragging, dragInput, dragStart, startPos
    TopBar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging, dragStart, startPos = true, input.Position, MainFrame.Position
            input.Changed:Connect(function() if input.UserInputState == Enum.UserInputState.End then dragging = false end end)
        end
    end)
    TopBar.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            dragInput = input
        end
    end)
    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and dragging then
            local delta = input.Position - dragStart
            TweenService:Create(MainFrame, TweenInfo.new(0.15, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
                Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
            }):Play()
        end
    end)

    CloseBtn.MouseButton1Click:Connect(function()
        TweenService:Create(MainScale, tweenInfo, {Scale = 0.8}):Play()
        local t = TweenService:Create(MainFrame, tweenInfo, {GroupTransparency = 1}) 
        t:Play() 
        t.Completed:Wait()
        MainFrame.Visible = false 
        OpenButton.Visible = true
        TweenService:Create(OpenButton, tweenInfo, {BackgroundTransparency = 0.5, TextTransparency = 0}):Play()
        TweenService:Create(OpenStroke, tweenInfo, {Transparency = 0.3}):Play()
    end)

    OpenButton.MouseButton1Click:Connect(function()
        TweenService:Create(OpenButton, tweenInfo, {BackgroundTransparency = 1, TextTransparency = 1}):Play()
        TweenService:Create(OpenStroke, tweenInfo, {Transparency = 1}):Play()
        task.wait(0.2) 
        OpenButton.Visible = false 
        MainFrame.Visible = true
        TweenService:Create(MainScale, tweenInfo, {Scale = 1}):Play()
        TweenService:Create(MainFrame, tweenInfo, {GroupTransparency = 0}):Play()
    end)

    task.spawn(function()
        if isfile(nombreAudio) then
            local s = Instance.new(loadstring(base64decode("U291bmQ="))(), ScreenGui)
            s.SoundId = getcustomasset(nombreAudio)
            s.Volume = 1.5
            s:Play()
            s.Ended:Connect(function() s:Destroy() end)
        end
        TweenService:Create(LoadingFrame, fadeInfo, {BackgroundTransparency = 0.15}):Play()
        TweenService:Create(LogoImage, fadeInfo, {ImageTransparency = 0}):Play()
        TweenService:Create(LoadingText, fadeInfo, {TextTransparency = 0}):Play()
        task.wait(2.5)
        TweenService:Create(LoadingFrame, fadeInfo, {BackgroundTransparency = 1}):Play()
        TweenService:Create(LogoImage, fadeInfo, {ImageTransparency = 1}):Play()
        TweenService:Create(LoadingText, fadeInfo, {TextTransparency = 1}):Play()
        task.wait(0.6)
        LoadingFrame:Destroy()
        MainFrame.Visible = true
        TweenService:Create(MainFrame, fadeInfo, {GroupTransparency = 0}):Play()
        TweenService:Create(MainScale, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Scale = 1}):Play()
    end)

    local Window = {}
    local activeTabBtn, activePage = nil, nil

    function Window:CreateTab(TabName)
        local btn = Instance.new(loadstring(base64decode("VGV4dEJ1dHRvbg=="))(), TabBar)
        btn.Size = UDim2.new(0, 100, 1, -5)
        btn.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        btn.BackgroundTransparency = 0.9
        btn.TextColor3 = Color3.fromRGB(200, 200, 200)
        btn.Font = Enum.Font.GothamMedium
        btn.TextSize = 12
        btn.Text = TabName
        btn.AutoButtonColor = false
        Instance.new(loadstring(base64decode("VUlDb3JuZXI="))(), btn).CornerRadius = UDim.new(0, 8)
        
        local page = Instance.new(loadstring(base64decode("U2Nyb2xsaW5nRnJhbWU="))(), PageContainer)
        page.Size = UDim2.new(1, 0, 1, 0)
        page.BackgroundTransparency = 1
        page.ScrollBarThickness = 2
        page.Visible = false
        local layout = Instance.new(loadstring(base64decode("VUlMaXN0TGF5b3V0"))(), page)
        layout.Padding = UDim.new(0, 8)
        layout.SortOrder = Enum.SortOrder.LayoutOrder

        layout:GetPropertyChangedSignal(loadstring(base64decode("QWJzb2x1dGVDb250ZW50U2l6ZQ=="))()):Connect(function()
            page.CanvasSize = UDim2.new(0, 0, 0, layout.AbsoluteContentSize.Y + 20)
        end)

        btn.MouseButton1Click:Connect(function()
            if activeTabBtn then
                activeTabBtn.BackgroundTransparency = 0.9
                activeTabBtn.TextColor3 = Color3.fromRGB(200, 200, 200)
                activePage.Visible = false
            end
            activeTabBtn = btn
            activePage = page
            btn.BackgroundTransparency = 0.2
            btn.TextColor3 = Color3.fromRGB(20, 20, 20)
            page.Visible = true
        end)

        if not activeTabBtn then 
            btn.BackgroundTransparency = 0.2 
            btn.TextColor3 = Color3.fromRGB(20, 20, 20) 
            page.Visible = true 
            activeTabBtn = btn 
            activePage = page 
        end

        local TabElements = {}

        function TabElements:CreateLabel(text)
            local lbl = Instance.new(loadstring(base64decode("VGV4dExhYmVs"))(), page)
            lbl.Size = UDim2.new(1, 0, 0, 20)
            lbl.BackgroundTransparency = 1
            lbl.Text = text
            lbl.TextColor3 = Color3.fromHex(loadstring(base64decode("I2Y5YmIzNQ=="))())
            lbl.Font = Enum.Font.GothamBold
            lbl.TextSize = 13
            lbl.TextXAlignment = Enum.TextXAlignment.Left
        end

        function TabElements:CreateButton(text, callback)
            local b = Instance.new(loadstring(base64decode("VGV4dEJ1dHRvbg=="))(), page)
            b.Size = UDim2.new(1, -5, 0, 32)
            b.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            b.BackgroundTransparency = 0.9
            b.TextColor3 = Color3.fromRGB(255, 255, 255)
            b.Font = Enum.Font.GothamMedium
            b.TextSize = 12
            b.Text = text
            Instance.new(loadstring(base64decode("VUlDb3JuZXI="))(), b).CornerRadius = UDim.new(0, 8)
            local stroke = Instance.new(loadstring(base64decode("VUlTdHJva2U="))(), b) 
            stroke.Color = Color3.fromRGB(255,255,255) 
            stroke.Transparency = 0.85
            b.MouseButton1Click:Connect(function() pcall(callback) end)
        end

        function TabElements:CreateToggle(text, callback)
            local state = false
            local tBtn = Instance.new(loadstring(base64decode("VGV4dEJ1dHRvbg=="))(), page)
            tBtn.Size = UDim2.new(1, -5, 0, 32)
            tBtn.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            tBtn.BackgroundTransparency = 0.9
            tBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
            tBtn.Font = Enum.Font.GothamMedium
            tBtn.TextSize = 12
            tBtn.Text = text
            Instance.new(loadstring(base64decode("VUlDb3JuZXI="))(), tBtn).CornerRadius = UDim.new(0, 8)
            local stroke = Instance.new(loadstring(base64decode("VUlTdHJva2U="))(), tBtn) 
            stroke.Color = Color3.fromRGB(255,255,255) 
            stroke.Transparency = 0.85

            tBtn.MouseButton1Click:Connect(function()
                state = not state
                tBtn.Text = state and (loadstring(base64decode("4pyTIA=="))() .. text) or text
                tBtn.TextColor3 = state and Color3.fromHex(loadstring(base64decode("I0Q0RTlEOA=="))()) or Color3.fromRGB(255, 255, 255)
                stroke.Color = state and Color3.fromHex(loadstring(base64decode("I0Q0RTlEOA=="))()) or Color3.fromRGB(255, 255, 255)
                pcall(callback, state)
            end)
        end

        function TabElements:CreateSlider(text, min, max, default, callback)
            local frame = Instance.new(loadstring(base64decode("RnJhbWU="))(), page)
            frame.Size = UDim2.new(1, -5, 0, 45)
            frame.BackgroundTransparency = 1

            local lbl = Instance.new(loadstring(base64decode("VGV4dExhYmVs"))(), frame)
            lbl.Size = UDim2.new(1, 0, 0, 15)
            lbl.BackgroundTransparency = 1
            lbl.Text = text .. loadstring(base64decode("OiA="))() .. default
            lbl.TextColor3 = Color3.fromRGB(255, 255, 255)
            lbl.Font = Enum.Font.GothamMedium
            lbl.TextSize = 12
            lbl.TextXAlignment = Enum.TextXAlignment.Left

            local bar = Instance.new(loadstring(base64decode("VGV4dEJ1dHRvbg=="))(), frame)
            bar.Size = UDim2.new(1, 0, 0, 8)
            bar.Position = UDim2.new(0, 0, 0, 25)
            bar.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            bar.BackgroundTransparency = 0.8
            bar.Text = loadstring(base64decode(""))()
            Instance.new(loadstring(base64decode("VUlDb3JuZXI="))(), bar).CornerRadius = UDim.new(1, 0)

            local fill = Instance.new(loadstring(base64decode("RnJhbWU="))(), bar)
            fill.Size = UDim2.new((default - min) / (max - min), 1, 0)
            fill.BackgroundColor3 = Color3.fromHex(loadstring(base64decode("I0Q0RTlEOA=="))())
            Instance.new(loadstring(base64decode("VUlDb3JuZXI="))(), fill).CornerRadius = UDim.new(1, 0)

            local dragging = false
            local function updateSlider(input)
                local pos = math.clamp((input.Position.X - bar.AbsolutePosition.X) / bar.AbsoluteSize.X, 0, 1)
                fill.Size = UDim2.new(pos, 0, 1, 0)
                local fAJC5WkC = math.floor(min + (max - min) * pos)
                lbl.Text = text .. loadstring(base64decode("OiA="))() .. fAJC5WkC
                pcall(callback, fAJC5WkC)
            end

            bar.InputBegan:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                    dragging = true updateSlider(input)
                end
            end)
            UserInputService.InputEnded:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then dragging = false end
            end)
            UserInputService.InputChanged:Connect(function(input)
                if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then updateSlider(input) end
            end)
        end

        function TabElements:CreateInput(placeholder, callback)
            local box = Instance.new(loadstring(base64decode("VGV4dEJveA=="))(), page)
            box.Size = UDim2.new(1, -5, 0, 32)
            box.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            box.BackgroundTransparency = 0.9
            box.TextColor3 = Color3.fromRGB(255, 255, 255)
            box.Font = Enum.Font.GothamMedium
            box.TextSize = 12
            box.PlaceholderText = placeholder
            box.Text = loadstring(base64decode(""))()
            box.ClearTextOnFocus = false
            Instance.new(loadstring(base64decode("VUlDb3JuZXI="))(), box).CornerRadius = UDim.new(0, 8)
            local stroke = Instance.new(loadstring(base64decode("VUlTdHJva2U="))(), box) 
            stroke.Color = Color3.fromRGB(255,255,255) 
            stroke.Transparency = 0.85
            
            box.FocusLost:Connect(function(enter) 
                if enter then pcall(callback, box.Text) end 
            end)
        end

        return TabElements, page
    end

    return Window
end

-- ==========================================
-- 3. INTEGRACIÓN DE HACKS (LA LÓGICA)
-- ==========================================
local Noclip, InfJump, AutoJump, ESPEnabled, NamesEnabled = false, false, false, false, false

local PlayRemote = ReplicatedStorage:WaitForChild(loadstring(base64decode("QW5pbWF0aW9uUmVwbGljYXRpb24="))(), 3) and ReplicatedStorage.AnimationReplication:WaitForChild(loadstring(base64decode("UGxheVNvdW5kQm9vbWJveA=="))(), 3)
local StopRemote = ReplicatedStorage:WaitForChild(loadstring(base64decode("QW5pbWF0aW9uUmVwbGljYXRpb24="))(), 3) and ReplicatedStorage.AnimationReplication:WaitForChild(loadstring(base64decode("U3RvcFNvdW5kQm9vbWJveA=="))(), 3)

local Menu = Library:CreateWindow(loadstring(base64decode("SGlkZSAmIFNlZWsgVWx0aW1hdGUgW3Y4XQ=="))())

-- PESTAÑA: JUGADOR
local TabJugador = Menu:CreateTab(loadstring(base64decode("SnVnYWRvcg=="))())
TabJugador:CreateLabel(loadstring(base64decode("TW92aW1pZW50bw=="))())
TabJugador:CreateSlider(loadstring(base64decode("VmVsb2NpZGFkIChXYWxrU3BlZWQp"))(), 16, 250, 16, function(fAJC5WkC)
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild(loadstring(base64decode("SHVtYW5vaWQ="))()) then 
        LocalPlayer.Character.Humanoid.WalkSpeed = fAJC5WkC 
    end
end)
TabJugador:CreateSlider(loadstring(base64decode("U2FsdG8gKEp1bXBQb3dlcik="))(), 50, 300, 50, function(fAJC5WkC)
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild(loadstring(base64decode("SHVtYW5vaWQ="))()) then
        LocalPlayer.Character.Humanoid.UseJumpPower = true
        LocalPlayer.Character.Humanoid.JumpPower = fAJC5WkC
    end
end)
TabJugador:CreateToggle(loadstring(base64decode("U2FsdG8gSW5maW5pdG8="))(), function(fAJC5WkC) InfJump = fAJC5WkC end)
TabJugador:CreateToggle(loadstring(base64decode("QXV0by1TYWx0bw=="))(), function(fAJC5WkC) AutoJump = fAJC5WkC end)
