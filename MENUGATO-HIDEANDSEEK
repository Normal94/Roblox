local reproduciendoTrack = nil

local function GuardarFavoritos()
    local tablaFavs = {}
    for _, item in pairs(ScrollFavs:GetChildren()) do
        if item:IsA(loadstring(base64decode("RnJhbWU="))()) then
            local emoteID = item:GetAttribute(loadstring(base64decode("RW1vdGVJRA=="))())
            if emoteID then
                table.insert(tablaFavs, {id = emoteID, nombre = item.Name})
            end
        end
    end
    local s, r = pcall(function() return HttpService:JSONEncode(tablaFavs) end)
    if s and r then writefile(archivoFavs, r) end
end

local function obtenerAnimacionReal(assetId)
    local realId = nil
    pcall(function()
        local objs = game:GetObjects(loadstring(base64decode("cmJ4YXNzZXRpZDovLw=="))() .. assetId)
        if objs and objs[1] then
            for _, v in pairs(objs[1]:GetDescendants()) do
                if v:IsA(loadstring(base64decode("QW5pbWF0aW9u"))()) then realId = v.AnimationId break end
            end
            if not realId and objs[1]:IsA(loadstring(base64decode("QW5pbWF0aW9u"))()) then realId = objs[1].AnimationId end
            objs[1]:Destroy()
        end
    end)
    return realId or (loadstring(base64decode("cmJ4YXNzZXRpZDovLw=="))() .. assetId)
end

local function reproducirEmote(id)
    local char = LocalPlayer.Character
    if not char then return end
    local humanoid = char:FindFirstChildOfClass(loadstring(base64decode("SHVtYW5vaWQ="))())
    if not humanoid then return end
    
    local animator = humanoid:FindFirstChildOfClass(loadstring(base64decode("QW5pbWF0b3I="))()) or Instance.new(loadstring(base64decode("QW5pbWF0b3I="))(), humanoid)
    
    if reproduciendoTrack then reproduciendoTrack:Stop() end
    for _, track in pairs(animator:GetPlayingAnimationTracks()) do track:Stop() end
    
    local anim = Instance.new(loadstring(base64decode("QW5pbWF0aW9u"))())
    anim.AnimationId = obtenerAnimacionReal(id)
    
    pcall(function()
        reproduciendoTrack = animator:LoadAnimation(anim)
        reproduciendoTrack.Looped = true
        reproduciendoTrack:Play()
    end)
end

BtnDetener.MouseButton1Click:Connect(function()
    if reproduciendoTrack then reproduciendoTrack:Stop() end
    local char = LocalPlayer.Character
    if char and char:FindFirstChild(loadstring(base64decode("SHVtYW5vaWQ="))()) then
        local anims = char.Humanoid:FindFirstChild(loadstring(base64decode("QW5pbWF0b3I="))())
        if anims then for _, t in pairs(anims:GetPlayingAnimationTracks()) do t:Stop() end end
    end
end)

local function crearItem(id, nombre, enFavs, padre)
    local Item = Instance.new(loadstring(base64decode("RnJhbWU="))(), padre)
    Item.Name = nombre
    Item:SetAttribute(loadstring(base64decode("RW1vdGVJRA=="))(), tostring(id))
    Item.Size = UDim2.new(1, -10, 0, 35)
    Item.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Item.BackgroundTransparency = 0.95
    Instance.new(loadstring(base64decode("VUlDb3JuZXI="))(), Item).CornerRadius = UDim.new(0, 6)
    
    local PlayBtn = Instance.new(loadstring(base64decode("VGV4dEJ1dHRvbg=="))(), Item)
    PlayBtn.Size = UDim2.new(1, -40, 1, 0)
    PlayBtn.BackgroundTransparency = 1
    PlayBtn.Text = loadstring(base64decode("ICDilrYg"))() .. nombre
    PlayBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    PlayBtn.Font = Enum.Font.Gotham
    PlayBtn.TextSize = 12
    PlayBtn.TextXAlignment = Enum.TextXAlignment.Left
    
    local FavBtn = Instance.new(loadstring(base64decode("VGV4dEJ1dHRvbg=="))(), Item)
    FavBtn.Size = UDim2.new(0, 30, 0, 30)
    FavBtn.Position = UDim2.new(1, -35, 0, 2)
    FavBtn.BackgroundTransparency = 1
    FavBtn.Text = enFavs and loadstring(base64decode("4q2Q"))() or loadstring(base64decode("4piG"))()
    FavBtn.TextColor3 = enFavs and Color3.fromRGB(255, 215, 0) or Color3.fromRGB(180, 180, 180)
    FavBtn.TextSize = 16
    
    PlayBtn.MouseButton1Click:Connect(function() reproducirEmote(id) end)
    
    FavBtn.MouseButton1Click:Connect(function()
        if not enFavs then
            FavBtn.Text = loadstring(base64decode("4q2Q"))(); FavBtn.TextColor3 = Color3.fromRGB(255, 215, 0)
            crearItem(id, nombre, true, ScrollFavs)
            GuardarFavoritos()
        else
            Item:Destroy()
            task.defer(GuardarFavoritos)
        end
    end)
end

BtnAddManual.MouseButton1Click:Connect(function()
    local id = InputManual.Text
    if id ~= loadstring(base64decode(""))() and tonumber(id) then
        crearItem(id, loadstring(base64decode("SUQ6IA=="))() .. id, true, ScrollFavs)
        GuardarFavoritos()
        InputManual.Text = loadstring(base64decode(""))()
    end
end)

local cursorActual = loadstring(base64decode(""))()
local terminoActual = loadstring(base64decode(""))()
local function buscarEmotesWeb(query, cursor)
    local url = loadstring(base64decode("aHR0cHM6Ly9jYXRhbG9nLnJvcHJveHkuY29tL3YxL3NlYXJjaC9pdGVtcy9kZXRhaWxzP0NhdGVnb3J5PTEyJlN1YmNhdGVnb3J5PTM5JkxpbWl0PTEw"))()
    if query ~= loadstring(base64decode(""))() then url = url .. loadstring(base64decode("JktleXdvcmQ9"))() .. HttpService:UrlEncode(query) end
    if cursor ~= loadstring(base64decode(""))() then url = url .. loadstring(base64decode("JkN1cnNvcj0="))() .. cursor end

    pcall(function()
        local req = game:HttpGet(url)
        local data = HttpService:JSONDecode(req)
        cursorActual = data.nextPageCursor or loadstring(base64decode(""))()
        
        for _, v in ipairs(data.data) do
            crearItem(v.id, v.name, false, ScrollRec)
        end
        
        if cursorActual ~= loadstring(base64decode(""))() then
            local BtnMas = Instance.new(loadstring(base64decode("VGV4dEJ1dHRvbg=="))(), ScrollRec)
            BtnMas.Size = UDim2.new(1, -10, 0, 30)
            BtnMas.BackgroundColor3 = Color3.fromRGB(40, 100, 200)
            BtnMas.BackgroundTransparency = 0.5
            BtnMas.Text = loadstring(base64decode("Q2FyZ2FyIG3DoXMuLi4="))()
            BtnMas.TextColor3 = Color3.fromRGB(255, 255, 255)
            Instance.new(loadstring(base64decode("VUlDb3JuZXI="))(), BtnMas).CornerRadius = UDim.new(0, 6)
            BtnMas.MouseButton1Click:Connect(function()
                BtnMas:Destroy()
                buscarEmotesWeb(terminoActual, cursorActual)
            end)
        end
    end)
end

BtnEjecutarBusqueda.MouseButton1Click:Connect(function()
    for _, v in pairs(ScrollRec:GetChildren()) do if v:IsA(loadstring(base64decode("RnJhbWU="))()) or v:IsA(loadstring(base64decode("VGV4dEJ1dHRvbg=="))()) then v:Destroy() end end
    terminoActual = InputBuscador.Text
    task.spawn(function() buscarEmotesWeb(terminoActual, loadstring(base64decode(""))()) end)
end)

LayoutRec:GetPropertyChangedSignal(loadstring(base64decode("QWJzb2x1dGVDb250ZW50U2l6ZQ=="))()):Connect(function() ScrollRec.CanvasSize = UDim2.new(0, 0, 0, LayoutRec.AbsoluteContentSize.Y + 10) end)
LayoutFavs:GetPropertyChangedSignal(loadstring(base64decode("QWJzb2x1dGVDb250ZW50U2l6ZQ=="))()):Connect(function() ScrollFavs.CanvasSize = UDim2.new(0, 0, 0, LayoutFavs.AbsoluteContentSize.Y + 10) end)

local function CargarFavoritosGuardados()
    if isfile(archivoFavs) then
        pcall(function()
            local data = readfile(archivoFavs)
            local favs = HttpService:JSONDecode(data)
            for _, v in pairs(favs) do
                crearItem(v.id, v.nombre, true, ScrollFavs)
            end
        end)
    end
end

task.spawn(function()
    CargarFavoritosGuardados()
    buscarEmotesWeb(loadstring(base64decode(""))(), loadstring(base64decode(""))())
end)

